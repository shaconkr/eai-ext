<?xml version='1.0' encoding='UTF-8' ?>
<beanio xmlns="http://www.beanio.org/2012/03"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <typeHandler type="java.util.Date" class="org.org.beanio.types.DateTypeHandler">
        <property name="pattern" value="yyyyMMddhhmm"/>
    </typeHandler>

    <stream kor="공통부" name="TOS_common" format="fixedlength">
        <parser>
            <property name="recordTerminator" value="\r\n"/>
            <property name="totalSize" value="120"/>
        </parser>
        <record name="record" class="map">
	        <field kor="식별코드" name="trCode" position="0" length="12"/>
	        <field kor="서비스구분" name="svcGb" position="12" length="3"/>
	        <field kor="송신자FLAG" name="sdFlag" position="15" length="1"/>
	        <field kor="송신자ID" name="sdId" position="16" length="10"/>
	        <field kor="수신자ID" name="rvId" position="26" length="10"/>
	        <field kor="메시지ID" name="msgId" position="36" length="12"/>
	        <field kor="고객코드" name="cpnyCd" position="48" length="8"/>
	        <field kor="은행코드" name="bnkCd" position="56" length="2"/>
	        <field kor="전문코드" name="ediCd" position="58" length="4"/>
	        <field kor="업무구분" name="jobGb" position="62" length="3"/>
	        <field kor="전문번호" name="ediNo" position="65" length="6"/>
	        <field kor="전송일자" name="sdDate" position="71" length="8"/>
	        <field kor="전송시간" name="sdTime" position="79" length="6"/>
	        <field kor="응답코드" name="retCd" position="85" length="4"/>
	        <field kor="예비영역" name="spareArea" position="89" length="31"/>
        </record>
    </stream>

    <stream kor="업무개시" name="TOS_0800_100" format="fixedlength">
        <parser>
            <property name="recordTerminator" value="\r\n"/>
        </parser>
        <record name="record" class="map">
            <field kor="예비" name="spareArea" position="0" length="120" padding=" "/>
            <field kor="은행전문번호" name="bnkEdiNo" position="120" length="6" padding=" "/>
            <field kor="자통법적용구분" name="jatongApplYn" position="127" length="1" padding=" "/>
            <field kor="공통부은행코드" name="commBnkCd" position="128" length="3" padding=" "/>
            <field kor="예비2" name="spareArea2" position="131" length="46" padding=" "/>            
        </record>
    </stream>

    <stream kor="당행이체요구" name="TOS_0600_100" format="fixedlength">
        <parser>
            <property name="recordTerminator" value="\r\n"/>
        </parser>
        <record name="record" class="map">
			<field kor="이체전문 전송일" name="ediSdDt" position="0" length="8" />
			<field kor="이체전문 전문번호" name="ediNo" position="8" length="6" />
			<field kor="모계좌번호" name="motherAccntNo" position="14" length="15" />
			<field kor="거래계좌 은행코드" name="transferBnkCd" position="29" length="2" />
			<field kor="거래계좌 번호" name="transferAccntNo" position="31" length="15" />
			<field kor="거래구분" name="transferGb" position="46" length="2" />
			<field kor="이체금액" name="transferAmt" position="48" length="13" />
			<field kor="이체처리시간" name="transferTime" position="61" length="6" />
			<field kor="이체 수수료" name="transferFee" position="67" length="9" />
			<field kor="cms코드" name="cmsCode" position="76" length="16" />
			<field kor="송금인 성명" name="payerNm" position="92" length="16" />
			<field kor="sub_seq" name="subSeq" position="108" length="6" />
			<field kor="이체처리결과" name="transferProcRet" position="114" length="4" />
			<field kor="타행처리번호" name="otherBnkProcNo" position="118" length="6" />
			<field kor="예비영역" name="spareArea" position="124" length="50" />
			<field kor="은행전문번호" name="bnkEdiNo" position="174" length="6" />
			<field kor="자통법적용구분" name="jatongApplYn" position="172" length="1" />
			<field kor="공통부 은행코드" name="commBnkCd" position="173" length="3" />
			<field kor="개별부 은행코드" name="indiBnkCd" position="176" length="3" />
			<field kor="예비영역2" name="spareArea2" position="179" length="43" />
        </record>
    </stream>

 

</beanio>